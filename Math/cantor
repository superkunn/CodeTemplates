int FAC[10]={1, 1, 2, 6, 24, 120, 720, 5040, 40320, 362880};
int cantor(string s,int n){
    int x=0;
    for(int i=0;i<n;i++){
        int smaller=0; // 在当前位之后小于其的个数
        for(int j=i+1;j<n;j++){
            if(s[j]<s[i])smaller++;// 康托展开累加
        }
        x+=FAC[n-i-1]*smaller;
    }
    return x;// 康托展开值
}
